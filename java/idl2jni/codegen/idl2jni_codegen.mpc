project: aceexe, crosscompile, dds_macros, install {
  sharedname   = idl2jni
  libout       = $(DDS_ROOT)/lib
  dynamicflags = IDL2JNI_BUILD_DLL

  requires += java
  after    += TAO_IDL_FE
  libs     += TAO_IDL_FE
  includes += . $(TAO_ROOT)/TAO_IDL/include $(TAO_ROOT)/TAO_IDL/fe $(TAO_ROOT) $(DDS_ROOT)
}
